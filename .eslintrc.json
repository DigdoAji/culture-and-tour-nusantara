{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "no-alert": "off",
        "linebreak-style": 0
    }
}
